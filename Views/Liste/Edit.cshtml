@{
    ViewData["Title"] = "Editer une liste";
}

<h1>@ViewData["Title"]</h1>

<div class="card">

    <form method="POST" asp-controller="Liste" asp-action="Edit" asp-route-IdListe="@Model.IdListe">

        <div class="input-group m-3">
            <p><input class="px-5" type="text" id="Nom" name="Nom" value="@Model.Nom"></p>
        </div>

        <div class="input-group m-3">
            <p><input class="px-5" type="text" id="IdProjet" name="IdProjet" value="@Model.IdProjet"></p>
        </div>

        <div class="input-group m-3">
            <input type="submit" value="Ajouter" />
        </div>

    </form>
</div>

